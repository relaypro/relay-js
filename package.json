{
  "name": "@relaypro/sdk",
  "version": "1.0.2",
  "description": "Workflow SDK Relay on Node.js",
  "license": "MIT",
  "repository": "github:relaypro/relay-js",
  "keywords": [
    "Relay Pro",
    "Relay",
    "Voice"
  ],
  "homepage": "https://api-docs.relaypro.com",
  "bugs": "https://github.com/relaypro/relay-js/issues",
  "main": "dist/index.js",
  "engines": {
    "node": ">=14.15.0"
  },
  "scripts": {
    "test": "nyc mocha --exit test/*",
    "release": "np",
    "build": "del-cli dist && tsc",
    "build:check": "tsc --noEmit",
    "prepare": "npm run build",
    "concat-doc": "mkdir -p concat-docs && npx concat-md --toc --decrease-title-levels --dir-name-as-title docs > concat-docs/relay-js.md",
    "doc-sync": "node ./scripts/doc-sync.mjs"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "tslib": "2.1.0",
    "ws": "7.4.4"
  },
  "devDependencies": {
    "@types/chai": "4.2.15",
    "@types/mocha": "8.2.2",
    "@types/node": "14.14.37",
    "@types/ws": "7.4.0",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "concat-md": "0.3.5",
    "del-cli": "3.0.1",
    "eslint": "7.23.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-mocha": "8.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "got": "11.8.2",
    "mocha": "8.3.2",
    "np": "7.4.0",
    "nyc": "15.1.0",
    "ts-node": "9.1.1",
    "typedoc": "0.20.34",
    "typedoc-plugin-markdown": "3.6.0",
    "typescript": "4.2.3"
  },
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
